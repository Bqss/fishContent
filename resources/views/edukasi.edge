                                                        viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                                        <path fill-rule="evenodd"
                                                            d="M8 4a4 4 0 100 8 4 4 0 000-8zM2 8a6 6 0 1110.89 3.476l4.817 4.817a1 1 0 01-1.414 1.414l-4.816-4.816A6 6 0 012 8z"
                                                            clip-rule="evenodd" />
                                                    </svg>
                                                </div>
                                                <input id="search"
                                                    class="block w-full text-gray-900 bg-white bg-opacity-20 py-2 pl-10 pr-3 border border-gray-300 rounded-md leading-3 focus:text-gray-900 placeholder-gray-900 focus:outline-none focus:bg-opacity-100 focus:border-transparent focus:placeholder-gray-500 focus:ring-0 sm:text-sm"
                                                    placeholder="Search" type="search" name="search">
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </section>
                    
                    <!-- courses section-->
                    <section>
                        <div class="w-full my-16 gap-6 grid grid-cols-1 md:grid-cols-2 2xl:grid-cols-3">
                            <!-- baris 1 -->
                            @each(course in courses)
                            @!component('components/course',{
                            ...course
                            })
                            @end

                        </div>
                        <!-- pagination -->
                        <div class="border-t bg-white border-gray-200 px-4 flex items-center justify-between ">
                            <div class="-mt-px w-0 flex-1 flex">
                                <a href={{`/edukasi/page/${id > 2 ? id-1 :id}`}}
                                    class="border-t-2 border-transparent py-4 pr-1 inline-flex items-center text-sm font-medium text-gray-500 hover:text-gray-700 hover:border-gray-300">
                                    <!-- Heroicon name: solid/arrow-narrow-left -->
                                    <svg class="mr-3 h-5 w-5 text-gray-400" xmlns="http://www.w3.org/2000/svg"
                                        viewBox="0 0 20 20" fill="currentColor" aria-hidden="true">
                                        <path fill-rule="evenodd"
                                            d="M7.707 14.707a1 1 0 01-1.414 0l-4-4a1 1 0 010-1.414l4-4a1 1 0 011.414 1.414L5.414 9H17a1 1 0 110 2H5.414l2.293 2.293a1 1 0 010 1.414z"
                                            clip-rule="evenodd" />
                                    </svg>
                                    Previous
                                </a>
                            </div>
                            <div class="hidden md:-mt-px md:flex">
                                @each((v, i) in pagination)
                                <a  class={{{` "text-gray-500 hover:text-gray-700  border-t-2 hover:border-gray-300  py-4 px-4 inline-flex items-center text-sm font-medium"`}}} href={{`/edukasi/page/${i+1}`}}>
                                    {{i+1}}</a>
                                @end


                            </div>
                            <div class="-mt-px w-0 flex-1 flex justify-end">
                                <a href={{`/edukasi/page/${id < pagination.length ? id +1 : id}`}} 
                                    class="border-t-2 border-transparent py-4 pl-1 inline-flex items-center text-sm font-medium text-gray-500 hover:text-gray-700 hover:border-gray-300">
